{
  "name": "y-markdown",
  "version": "0.0.13",
  "description": "render markdown in browser",
  "main": "lib/index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "babel entry --out-dir lib && npx node-sass -r entry -o lib --output-style compressed",
    "webpack": "webpack",
    "publish": "npm publish --registry http://registry.npmjs.org",
    "publish:patch": "npm version patch && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:major": "npm version major && npm publish"
  },
  "keywords": [
    "markdown"
  ],
  "author": "ylvfeng",
  "license": "ISC",
  "dependencies": {
    "clsx": "^1.1.1",
    "highlight.js": "^10.4.1",
    "marked": "^0.6.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^0.23.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.22.0",
    "markdown-loader": "^5.0.0",
    "node-sass": "^6.0.1",
    "raw-loader": "^4.0.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.13.2",
    "webpack": "2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
